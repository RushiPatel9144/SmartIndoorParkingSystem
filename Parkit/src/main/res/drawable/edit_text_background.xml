<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Background color for the EditText -->
    <solid android:color="#FFFFFF" />

    <!-- Border color and width -->
    <stroke
        android:width="1dp"
        android:color="#CCCCCC" />

    <!-- Corner radius to make rounded edges -->
    <corners android:radius="8dp" />

    <!-- Padding inside the EditText -->
    <padding
        android:left="8dp"
        android:right="8dp"
        android:top="8dp"
        android:bottom="8dp" />
</shape>
